    <main>
        <!-- breadcrumb area start -->
        <div class="breadcrumb-area breadcrumb-img bg-img" data-bg="assets/img/banner/sharma.jpg">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="breadcrumb-wrap">
                            <nav aria-label="breadcrumb">
                                <h3 class="breadcrumb-title">SHOP</h3>
                                <ul class="breadcrumb justify-content-center">
                                    <li class="breadcrumb-item"><a href="/"><i class="fa fa-home"></i></a></li>
                                    <li class="breadcrumb-item"><a href="/shop?id=1">Shop</a></li>
                                    <li class="breadcrumb-item active" aria-current="page">Product Details</li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- breadcrumb area end -->

        <!-- page main wrapper start -->
        <div class="shop-main-wrapper section-padding pb-0">
            <div class="container">
                <div class="row">
                    <!-- product details wrapper start -->
                    <div class="col-lg-12 order-1 order-lg-2">
                        <!-- product details inner end -->
                        <div class="product-details-inner">
                            <div class="row">
                                <div class="col-lg-5">
                                    <div class="product-large-slider">
                                        <% images.forEach(li => { %>
                                        <div class="pro-large-img img-zoom">
                                            <img src="assets/img/product/MyProductImages/<%=li %>.png" alt="product-details" />
                                        </div>
                                        <% }); %>
                                    </div>
                                    <div class="pro-nav slick-row-10 slick-arrow-style">
                                        <% images.forEach(si => { %>
                                        <div class="pro-nav-thumb">
                                            <img src="assets/img/product/MyProductImages/<%=si %>.png" alt="product-details" />
                                        </div>
                                        <% }); %>
                                    </div>
                                </div>
                                <div class="col-lg-7">
                                    <div class="product-details-des">
                                        <h3 class="product-name"><%=result[0].product_title; %></h3>
                                        <div class="price-box">
                                            <% var avar=result[0].product_price*(result[0].product_offer/100); 
                                                            var adiscount=result[0].product_price - avar; %>
                                                                <% if(result[0].product_offer !=0){ %>
                                                                    <span class="price-old"><del>&#8377;<%=result[0].product_price; %></del></span>
                                                                    <% } %>
                                                                    <span class="price-regular">&#8377;<%=adiscount; %></span>
                                        </div>
                                        <div class="availability">
                                            <i class="fa fa-check-circle"></i>
                                            <span id="productstockForProductDetails"><%=result[0].product_stock; %> in stock</span>
                                        </div>
                                        <p class="pro-desc"><%=result[0].product_des; %></p>
                                        <div class="quantity-cart-box d-flex align-items-center">
                                            <h6 class="option-title">qty:</h6>
                                            <div class="quantity">
                                                <div class="pro-qty"><input id="quickViewInputQuantity" type="text" value="1"></div>
                                            </div>
                                            <input id="quickViewHiddenInputSetProductId" value="<%=result[0].product_id; %>" type="hidden">
                                            <div class="action_link">
                                                <a class="btn btn-cart2" onclick="addProductIntoCartFromQuickView()">Add To Cart</a>
                                            </div>
                                        </div>
                                        <div class="pro-size">
                                            <h6 class="option-title">size :</h6>
                                            <select class="nice-select">
                                                <% if(result[0].product_size_SM == "true"){ %>
                                                <option>S</option>
                                                <% } if(result[0].product_size_MD == "true"){ %>
                                                <option>M</option>
                                                <% } if(result[0].product_size_LG == "true"){ %>
                                                <option>L</option>
                                                <% } if(result[0].product_size_XL == "true"){ %>
                                                <option>XL</option>
                                                <% } %>
                                            </select>
                                        </div>
                                        <div class="color-option">
                                            <h6 class="option-title">color :</h6>
                                            <ul class="color-categories">
                                                <input type="hidden" id="colors" value="<%=colors; %>">
                                                <% var i = 1;
                                                 colors.forEach(c => { %>
                                                    <li>
                                                        <a id="setColor<%=i; %>" href="#" title="<%=c; %>"></a>
                                                    </li>
                                                <% i++; }); %>
                                            </ul>
                                        </div>
                                        <div class="useful-links">
                                            <a style="cursor: pointer;" onclick="addWishList('<%=result[0].product_id; %>')" data-toggle="tooltip" title="Wishlist"><i
                                                class="fa fa-heart-o"></i>wishlist</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- product details inner end -->

                        <!-- product details reviews start -->
                        <div class="product-details-reviews section-padding pb-0">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="product-review-info">
                                        <ul class="nav review-tab">
                                            <li>
                                                <a class="active" data-toggle="tab" href="#tab_one">description</a>
                                            </li>
                                            <li>
                                                <a data-toggle="tab" href="#tab_two">information</a>
                                            </li>
                                        </ul>
                                        <div class="tab-content reviews-tab">
                                            <div class="tab-pane fade show active" id="tab_one">
                                                <div class="tab-one">
                                                    <p><%=result[0].product_des; %>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="tab-pane fade" id="tab_two">
                                                <table class="table table-bordered">
                                                    <tbody>
                                                        <tr>
                                                            <td>Color</td>
                                                            <td><%=colors; %></td>
                                                        </tr>
                                                        <tr>
                                                            <td>Size</td>
                                                            <td>
                                                                <% if(result[0].product_size_SM == "true"){ %>
                                                                S,
                                                                <% } if(result[0].product_size_MD == "true"){ %>
                                                                M,
                                                                <% } if(result[0].product_size_LG == "true"){ %>
                                                                L,
                                                                <% } if(result[0].product_size_XL == "true"){ %>
                                                                XL
                                                                <% } %>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td>Company</td>
                                                            <td><%=result[0].product_company; %></td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- product details reviews end -->
                    </div>
                    <!-- product details wrapper end -->
                </div>
            </div>
        </div>
        <!-- page main wrapper end -->

        <!-- Related product area start -->
        <section class="product-gallery section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="section-title text-center">
                            <h3 class="title">RELATED PRODUCT</h3>
                            <h4 class="sub-title">Investigationes demonstraverunt lectores legere me lius quod ii legunt saepius claritas est etiam processus dynamicus, qui sequitur mutationem.</h4>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="product-carousel--4 slick-row-5 slick-arrow-style">
                            <!-- product single item start -->
                            <% var i = 0;
                            rresult.forEach(rp => { %>
                            <div class="product-item">
                                <div class="product-thumb">
                                    <a href="/product-details?id=<%=rp.product_id; %>">
                                        <img src="assets/img/product/MyProductImages/<%=rimages[i][2]; %>.png" alt="product thumb">
                                    </a>
                                    <div class="button-group">
                                        <a href="wishlist.html" data-toggle="tooltip" title="Add to Wishlist"><i class="fa fa-heart-o"></i></a>
                                        <a href="#" data-toggle="modal"
                                        onclick="quickView('<%=rp.product_id %>','<%=rp.product_title; %>','<%=rp.product_des; %>','<%=rp.product_price; %>','<%=rp.product_offer; %>','<%=rimages[i][0]; %>','<%=rimages[i][1]; %>','<%=rimages[i][2]; %>','<%=rimages[i][3]; %>')" data-target="#quick_view"><span data-toggle="tooltip"  title="Quick View"><i class="fa fa-eye"></i></span></a>
                                    </div>
                                    <div class="product-label">
                                        <span>new</span>
                                    </div>
                                    <% if(rp.product_offer != 0){ %>
                                    <div class="discount-label">
                                        <span>-<%=rp.product_offer; %> Off</span>
                                    </div>
                                    <% } %>
                                </div>
                                <div class="product-content">
                                    <div class="product-caption">
                                        <h6 class="product-name">
                                            <a href="/product-details?id=<%=rp.product_id; %>"><%=rp.product_title; %></a>
                                        </h6>
                                        <div class="price-box">
                                            <% var avar=rp.product_price*(rp.product_offer/100); 
                                                            var adiscount=rp.product_price - avar; %>
                                                                <% if(rp.product_offer !=0){ %>
                                                                    <span class="price-old"><del>$<%=rp.product_price; %></del></span>
                                                                    <% } %>
                                                                    <span class="price-regular">$<%=adiscount; %></span>
                                        </div>
                                        <a style="cursor: pointer;" class="add-to-cart" onclick="addProductIntoCart('<%=rp.product_id; %>')"><i class="fa fa-shopping-cart"></i></a>
                                    </div>
                                </div>
                            </div>
                            <% i++; }); %>
                            <!-- product single item end -->
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- Related product area end -->
    </main>

