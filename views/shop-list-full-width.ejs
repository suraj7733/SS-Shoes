                    <!-- shop main wrapper start -->
                    <div class="col-lg-12">
                        <div class="shop-product-wrapper">
                            <!-- shop product top wrap start -->
                            <div class="shop-top-bar">
                                <div class="row align-items-center">
                                    <div class="col-lg-7 col-md-6 order-2 order-md-1">
                                        <div class="top-bar-left">
                                            <div class="product-amount">
                                               <% if(u == 1){ %>
                                                <p><%=len; %> results</p>
                                               <% }else if(last > len){ %>
                                                <p>Showing <%=first+1; %>–<%=(last-1); %> of <%=len; %> results</p>
                                               <% }else{ %>
                                                <p>Showing <%=first+1; %>–<%=(last); %> of <%=len; %> results</p>
                                               <% } %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- shop product top wrap start -->

                            <!-- product item list wrapper start -->
                            <div class="shop-product-wrap list-view row mbn-30">
                                <!-- product single item start -->
                                <div class="col-lg-3 col-md-4 col-sm-6">
                                    <!-- product list item end -->
                                    <% var i = 0;
                                    result.forEach(r => { %>
                                    <div class="product-list-item">
                                        <div class="product-thumb">
                                            <a href="/product-details?id=<%=r.product_id; %>">
                                                <img src="<%='assets/img/product/MyProductImages/'+images[i][0]+'.png'; %>" alt="product thumb">
                                            </a>
                                            <div class="button-group">
                                                <a href="#" data-toggle="tooltip"
                                                   onclick="addWishList('<%=r.product_id %>')" title="Add to Wishlist"><i class="fa fa-heart-o"></i></a>
                                                <a href="#" data-toggle="modal" data-target="#quick_view"><span
                                                     onclick="quickView('<%=r.product_id; %>','<%=r.product_title; %>','<%=r.product_des; %>','<%=r.product_price; %>','<%=r.product_offer; %>','<%=images[i][0]; %>','<%=images[i][1]; %>','<%=images[i][2]; %>','<%=images[i][3]; %>')"   data-toggle="tooltip" title="Quick View"><i
                                                            class="fa fa-eye"></i></span></a>
                                            </div>
                                            <div class="product-label">
                                                <span>new</span>
                                            </div>
                                            <% if(r.product_offer !=0){ %>
                                                <div class="discount-label">
                                                    <span>-<%=r.product_offer; %>% Off</span>
                                                </div>
                                                <% } %>
                                        </div>
                                        <div class="product-content-list">
                                            <h4 class="product-name"><a href="/product-details?id=<%=r.product_id; %>"><%=r.product_title; %></a></h4>
                                            <div class="price-box">
                                                <% var avar=r.product_price*(r.product_offer/100); 
                                                var adiscount=r.product_price - avar; %>
                                                    <% if(r.product_offer !=0){ %>
                                                        <span class="price-old"><del>$<%=r.product_price; %></del></span>
                                                        <% } %>
                                                        <span class="price-regular">$<%=adiscount; %></span>
                                            </div>
                                            <p><%=r.product_des; %>
                                            </p>
                                            <a class="btn btn-large hover-color" style="cursor: pointer;" onclick="addProductIntoCart('<%=r.product_id; %>')">Add To Cart</a>
                                        </div>
                                    </div>
                                    <br><hr>
                                    <% i++; }); %>
                                    <!-- product list item end -->
                                </div>
                                <!-- product single item start -->
                            </div>
                            <!-- product item list wrapper end -->

                            <!-- start pagination area parseInt(i.toString().split(".")[0]) -->
                            <% if(u == 0){ %>
                            <div class="paginatoin-area text-center">
                                <ul class="pagination-box">
                                    <% var c = parseInt((len/3).toString().split(".")[0]); %>
                                    <li id="active1"><a href="/shop?id=1">1</a></li>
                                    <% 
                                        var p = 2;
                                    if(c > 1){ 
                                       while(p <= c){ %>
                                    <li id="active<%=p; %>"><a href="/shop?id=<%=p; %>"><%=p; %></a></li>
                                    <% p++; }
                                      }; %>
                                    <% var d = parseInt((len/3).toString().split(".")[1]); 
                                       if(d > 0){ %>
                                    <li id="active<%=p; %>"><a href="/shop?id=<%=p; %>"><%=p; %></a></li>
                                    <% } %>
                                </ul>
                            </div>
                            <% } %>
                            <!-- end pagination area -->
                        </div>
                    </div>
                    <!-- shop main wrapper end -->
                </div>
